.clock {
  position: relative;
  background: var(--greenLinear);
  color: #121212;
  border-radius: 3px;
  display: grid;
  place-content: center;
  height: 60px;
  margin: 0;
  width: 180px;
  font-size: 2.25rem;
  font-family: "Digital-7 Mono Mono";
}

.clock:after {
  background-color: rgba(255, 255, 255, 0.15);
  border-radius: 4px;
  content: "";
  height: 40%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 1;
}

.ghost {
  position: absolute;
  color: hsla(118, 60%, 63%, 0.1);
  border-radius: 3px;
  display: grid;
  place-content: center;
  height: 60px;
  margin: 0;
  width: 180px;
  font-size: 2.25rem;
  font-family: "Digital-7 Mono Mono";
}
.channel {
  display: flex;
  flex-direction: column;
  /* gap: 8px; */
  justify-content: space-between;
}
.song-select {
  font-family: "Tekton";
  font-weight: bold;
  font-size: 1.2rem;
  letter-spacing: 0.05rem;
  background: none;
  border: 1px solid #efefef;
  color: #efefef;
  width: 450px;
  padding: 12px;
  border-radius: 4px 0 0 4px;
  outline: none;
}

.submit-btn {
  line-height: 1.25;
  font-family: "Tekton";
  font-weight: bold;
  font-size: 1.2rem;
  letter-spacing: 0.05rem;
  background: none;
  color: #efefef;
  border: 1px solid #efefef;
  border-radius: 0 4px 4px 0;
  padding: 12px;
  transition: all 0.25s;
  outline: none;
  width: 12ch;
}
.submit-btn:hover {
  background: hsl(255 100% 100%/0.5);
  font-weight: bold;
  color: hsl(0, 0%, 12.5%);
}
.submit-btn[disabled]:hover {
  background: hsl(255 100% 100%/0);
  font-weight: bold;
  color: #efefef;
}
.song-info {
  font-size: 1.25rem;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  margin-bottom: 16px;
}

