body {
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto",
    "Droid Sans", "Helvetica Neue", sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  max-width: 100vw;
  overflow-x: hidden;
  color: #efefef;
  height: 100vh;
  font-family: "Tekton";
  background: var(--background);
}

a {
  color: inherit;
  text-decoration: none;
}

h1 {
  font-size: 36px;
  line-height: 40px;
  font-weight: bold;
}

h2 {
  font-size: 30px;
  line-height: 40px;
  font-weight: bold;
}

h3 {
  font-size: 24px;
  line-height: 40px;
  font-weight: bold;
}

h4 {
  font-size: 18px;
  line-height: 20px;
  font-weight: bold;
}

h1,
h2 {
  margin: 1rem 0;
}

h3,
h4 {
  margin: 0.5rem 0;
}

.layout-wrap {
  display: grid;
  grid-template-rows: auto 1fr 5%;
  justify-content: center;
  height: 100vh;
}

ol {
  list-style-type: decimal;
}

.user-mix-list,
.community-mix-list {
  max-width: 300px;
  max-height: 300px;
  position: relative;
  border: 1px solid hsl(0, 0%, 100%, 0.25);
  padding: 12px;
  font-size: 1.1rem;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  aspect-ratio: 1/1;
  background-size: 75%;
  background-repeat: no-repeat;
  background-position: center;
}

.user-mix-list {
  filter: contrast(100%) brightness(59%) grayscale(50%) hue-rotate(234deg);
  background-blend-mode: luminosity;
}

.community-mix-list {
  background-blend-mode: multiply;
}

.vertical-gap {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  gap: 48px;
}

.vertical-space-evenly {
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  align-items: center;
  height: 100%;
}

.vertical-space-between {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 100%;
}
.user-mix-list a,
.community-mix-list a {
  color: #efefef;
  text-decoration: none;
  font-weight: 500;
  padding: 4px;
}

.community-mix-list-container,
.user-mix-list-container {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  max-width: 100%;
  width: 1024px;
  list-style: none;
  grid-gap: 24px;
}

.community-mix-list-container li > div,
.user-mix-list-container li > div {
  border: 1px solid #ededed;
  border-radius: 4px;
}

.user-mix-list-container li:nth-of-type(5n - 1) > div {
  background-color: var(--pink-transparent);
}

.user-mix-list-container li:nth-of-type(5n - 2) > div {
  background-color: var(--blue-transparent);
}

.user-mix-list-container li:nth-of-type(5n - 3) > div {
  background-color: var(--yellow-transparent);
}

.user-mix-list-container li:nth-of-type(5n - 4) > div {
  background-color: var(--red-transparent);
}

.user-mix-list-container li:nth-of-type(5n - 5) > div {
  background-color: var(--green-transparent);
}

.user-mix-list-container li > div:hover {
  background-color: transparent;
}

.community-mix-list-container li:nth-of-type(5n + 1) > div {
  background-color: var(--pink-transparent);
}

.community-mix-list-container li:nth-of-type(5n + 2) > div {
  background-color: var(--blue-transparent);
}

.community-mix-list-container li:nth-of-type(5n + 3) > div {
  background-color: var(--yellow-transparent);
}

.community-mix-list-container li:nth-of-type(5n + 4) > div {
  background-color: var(--red-transparent);
}

.community-mix-list-container li:nth-of-type(5n + 5) > div {
  background-color: var(--green-transparent);
}

.community-mix-list-container li > div:hover,
.user-mix-list-container li > div:hover {
  background-color: transparent;
}

.mixer {
  position: relative;
  display: flex;
  gap: 0.25px;
  flex-wrap: wrap;
  justify-content: center;
  align-items: flex-end;
  max-width: 82.5vw;
}

