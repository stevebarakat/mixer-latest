:root {
  --background: hsl(0, 0%, 7%);
  --blue: hsl(213, 100%, 61%);
  --green: hsl(118, 60%, 63%);
  --yellow: hsl(47, 99%, 55%);
  --pink: hsl(302, 67%, 54%);
  --red: hsl(355, 89%, 61%);

  --blue-transparent: hsl(213 100% 61%/0.25);
  --green-transparent: hsl(118 60% 63%/0.25);
  --yellow-transparent: hsl(47 99% 55%/0.25);
  --pink-transparent: hsl(302 67% 54%/0.25);
  --red-transparent: hsl(355 89% 61%/0.25);

  --blueLinear: linear-gradient(
    to bottom,
    hsl(213, 100%, 71%) 0%,
    hsl(213, 100%, 61%) 100%
  );

  --blueLinearTop: linear-gradient(
    to bottom,
    hsl(213, 100%, 81%) 0%,
    hsl(213, 100%, 75%) 100%
  );

  --blueLinearBtm: linear-gradient(
    to bottom,
    hsl(213, 100%, 75%) 0%,
    hsl(213, 100%, 71%) 100%
  );

  --greenLinear: linear-gradient(
    to bottom,
    hsl(118, 60%, 73%) 0%,
    hsl(118, 60%, 63%) 100%
  );
  --greenLinearTop: linear-gradient(
    to bottom,
    hsl(118, 60%, 83%) 0%,
    hsl(118, 60%, 77%) 100%
  );
  --greenLinearBtm: linear-gradient(
    to bottom,
    hsl(118, 60%, 77%) 0%,
    hsl(118, 60%, 73%) 100%
  );
  --yellowLinear: linear-gradient(
    to bottom,
    hsl(47, 99%, 75%) 0%,
    hsl(47, 99%, 55%) 100%
  );
  --yellowLinearTop: linear-gradient(
    to bottom,
    hsl(47, 99%, 75%) 0%,
    hsl(47, 99%, 70%) 100%
  );
  --yellowLinearBtm: linear-gradient(
    to bottom,
    hsl(47, 99%, 70%) 0%,
    hsl(47, 99%, 65%) 100%
  );
  --yellowLinearBtmTransparent: linear-gradient(
    to bottom,
    hsl(47, 99%, 70%, 0.95) 0%,
    hsl(47, 99%, 65%, 0.95) 100%
  );
  --pinkLinear: linear-gradient(
    to bottom,
    hsl(302, 67%, 74%) 0%,
    hsl(302, 67%, 54%) 100%
  );
  --pinkLinearTop: linear-gradient(
    to bottom,
    hsl(302, 67%, 74%) 0%,
    hsl(302, 67%, 69%) 100%
  );
  --pinkLinearBtm: linear-gradient(
    to bottom,
    hsl(302, 67%, 69%) 0%,
    hsl(302, 67%, 64%) 100%
  );
  --redLinear: linear-gradient(
    to bottom,
    hsl(355, 89%, 71%) 0%,
    hsl(355, 89%, 51%) 100%
  );
  --redLinearTop: linear-gradient(
    to bottom,
    hsl(355, 89%, 81%) 0%,
    hsl(355, 89%, 76%) 100%
  );
  --redLinearBtm: linear-gradient(
    to bottom,
    hsl(355, 89%, 76%) 0%,
    hsl(355, 89%, 71%) 100%
  );

  --transparent-black: hsl(0 0% 0%/0.75);
  --transparent-white: hsl(255 0% 100%/0.75);

  --shadow-color: 0deg 0% 0%;
  --shadow-elevation-low: 0.3px 0.5px 0.7px hsl(var(--shadow-color) / 0.1),
    0.4px 0.8px 1px -1.2px hsl(var(--shadow-color) / 0.1),
    1px 2px 2.5px -2.5px hsl(var(--shadow-color) / 0.1);
  --shadow-elevation-medium: 0.3px 0.5px 0.7px hsl(var(--shadow-color) / 0.11),
    0.8px 1.6px 2px -0.8px hsl(var(--shadow-color) / 0.11),
    2.1px 4.1px 5.2px -1.7px hsl(var(--shadow-color) / 0.11),
    5px 10px 12.6px -2.5px hsl(var(--shadow-color) / 0.11);
  --shadow-elevation-high: 0.3px 0.5px 0.7px hsl(var(--shadow-color) / 0.1),
    1.5px 2.9px 3.7px -0.4px hsl(var(--shadow-color) / 0.1),
    2.7px 5.4px 6.8px -0.7px hsl(var(--shadow-color) / 0.1),
    4.5px 8.9px 11.2px -1.1px hsl(var(--shadow-color) / 0.1),
    7.1px 14.3px 18px -1.4px hsl(var(--shadow-color) / 0.1),
    11.2px 22.3px 28.1px -1.8px hsl(var(--shadow-color) / 0.1),
    17px 33.9px 42.7px -2.1px hsl(var(--shadow-color) / 0.1),
    25px 50px 62.9px -2.5px hsl(var(--shadow-color) / 0.1);
}

