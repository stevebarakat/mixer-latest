.channel:nth-of-type(5n + 5) .simple-range-labels {
  text-shadow: 0px 2px 3px hsl(355, 89%, 81%);
}
.track-labels {
  width: 100px;
  border-radius: 0 0 4px 4px;
  display: flex;
  justify-content: center;
  font-weight: 900;
  padding: 4px;
  color: var(--transparent-black);
  text-shadow: 0px 2px 3px hsl(355, 89%, 81%);
  font-family: "Tekton";
}

.track-labels {
  background: var(--redLinear);
}
.track-label {
  background: var(--yellowLinear);
}
.channel:nth-of-type(5n + 1) .track-labels {
  background: var(--blueLinear);
  text-shadow: 0px 2px 3px hsl(213, 100%, 77%);
}
.channel:nth-of-type(5n + 2) .track-labels {
  background: var(--greenLinear);
  text-shadow: 0px 2px 3px hsl(118, 60%, 85%);
}
.channel:nth-of-type(5n + 3) .track-labels {
  background: var(--pinkLinear);
  text-shadow: 0px 2px 3px hsl(302, 67%, 79%);
}
.channel:nth-of-type(5n + 4) .track-labels {
  background: var(--yellowLinear);
  text-shadow: 0px 2px 3px hsl(47, 99%, 80%);
}
.channel:nth-of-type(5n + 5) .track-labels {
  background: var(--redLinear);
  text-shadow: 0px 2px 3px hsl(355, 89%, 81%);
}

.label {
  display: inline-block;
  background: linear-gradient(
    to bottom,
    hsla(222, 30%, 80%, 1) 0%,
    hsla(0, 0%, 50%, 1) 100%
  );
  color: #121212;
  border-radius: 4px;
  white-space: nowrap;
  user-select: none;
  -webkit-tap-highlight-color: transparent;
  transition: all 0.2s;
  padding: 8px 12px;
  cursor: pointer;
  width: 100%;
  text-align: center;
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto",
    "Droid Sans", "Helvetica Neue", sans-serif;
}

.channel:nth-of-type(5n + 1) .chan-strip-btn .label {
  background: var(--blueLinearTop);
}

.channel:nth-of-type(5n + 1) .pfl .label {
  background: var(--blueLinearBtm);
}

.channel:nth-of-type(5n + 2) .chan-strip-btn .label {
  background: var(--greenLinearTop);
}

.channel:nth-of-type(5n + 2) .pfl .label {
  background: var(--greenLinearBtm);
}

.channel:nth-of-type(5n + 3) .chan-strip-btn .label {
  background: var(--pinkLinearTop);
}

.channel:nth-of-type(5n + 3) .pfl .label {
  background: var(--pinkLinearBtm);
}

.channel:nth-of-type(5n + 4) .chan-strip-btn .label {
  background: var(--yellowLinearTop);
}

.channel:nth-of-type(5n + 4) .pfl .label {
  background: var(--yellowLinearBtm);
}

.channel:nth-of-type(5n + 5) .chan-strip-btn .label {
  background: var(--redLinearTop);
}

.channel:nth-of-type(5n + 5) .pfl .label {
  background: var(--redLinearBtm);
}

.bus input[type="checkbox"]:checked + .label {
  background-color: #12bbd4;
  color: var(--yellow);
  transition: all 0.2s;
}

.busses-wrap {
  display: flex;
  flex-direction: column;
  gap: 4px;
}

.busses-wrap:first-child {
  margin-left: 8px;
}

.controls input[type="radio"] + .label {
  background: var(--redLinearBtm);
  color: #dedede;
  width: 20px;
  /* height: 20px; */
  display: grid;
  place-items: center;
  font-weight: 700;
  font-size: 0.5rem;
}

.controls input[type="radio"]:checked + .label,
.chan-strip-btn input[type="checkbox"]:checked + .label {
  background-color: #12bbd4;
  color: var(--yellow);
  transition: all 0.2s;
}

.channel:nth-of-type(5n + 1)
  .chan-strip-btn
  input[type="checkbox"]:checked
  + .label,
.channel:nth-of-type(5n + 2)
  .chan-strip-btn
  input[type="checkbox"]:checked
  + .label,
.channel:nth-of-type(5n + 4)
  .chan-strip-btn
  input[type="checkbox"]:checked
  + .label {
  color: var(--red);
  transition: all 0.2s;
}

input[type="checkbox"],
.controls input[type="radio"] {
  position: absolute;
  opacity: 0;
}

.chan-strip-btn {
  font-weight: 900;
  font-size: 1.5rem;
  width: 50px;
}

.channel:nth-of-type(5n + 1) .chan-strip-btn {
  text-shadow: 0px 2px 3px hsl(213, 100%, 93%);
}
.channel:nth-of-type(5n + 1) .pfl {
  text-shadow: 0px 2px 3px hsl(213, 100%, 88%);
}

.channel:nth-of-type(5n + 2) .chan-strip-btn {
  text-shadow: 0px 2px 3px hsl(118, 60%, 98%);
}
.channel:nth-of-type(5n + 2) .pfl {
  text-shadow: 0px 2px 3px hsl(118, 60%, 93%);
}

.channel:nth-of-type(5n + 3) .chan-strip-btn {
  text-shadow: 0px 2px 3px hsl(302, 67%, 81%);
}
.channel:nth-of-type(5n + 3) .pfl {
  text-shadow: 0px 2px 3px hsl(302, 67%, 74%);
}

.channel:nth-of-type(5n + 4) .chan-strip-btn {
  text-shadow: 0px 2px 3px hsl(47, 99%, 95%);
}
.channel:nth-of-type(5n + 4) .pfl {
  text-shadow: 0px 2px 3px hsl(47, 99%, 90%);
}

.channel:nth-of-type(5n + 5) .chan-strip-btn {
  text-shadow: 0px 2px 3px hsl(355, 89%, 83%);
}
.channel:nth-of-type(5n + 5) .pfl {
  text-shadow: 0px 2px 3px hsl(355, 89%, 80%);
}

.level-val {
  width: 60px;
  height: fit-content;
  border: none;
  outline: none;
  font-size: 16px;
  text-align: center;
  background: none;
}

.levels-wrap {
  padding: 24px;
  border-radius: 8px;
  box-shadow: inset 0 2px 3px black, inset 0 2px 8px black;
}

/* paint background of level meter */
.levels {
  height: 300px;
  width: 20px;
  background-color: #333;
  border-radius: 2px;
  display: flex;
  flex-direction: row;
  padding: 4px 3px;
  margin: auto;
  margin-top: 20px;
}

/* LEDs off */
.level {
  position: relative;
  margin: 0 2px;
  flex-grow: 1;
  z-index: 0;
  background-color: transparent;
  background-repeat: repeat-y;
  background-image: linear-gradient(to bottom, #666 3px, transparent 1px);
  background-size: 100% 4px;
}

/* LEDs on */
.glow {
  width: 100%;
  height: 100%;
  content: " ";
  position: absolute;
  top: 0px;
  left: 0px;
  z-index: 1;
  background-repeat: no-repeat;
  background-image: linear-gradient(to top, green 0%, yellow 80%, red 100%);
  background-size: 100% 100%;
}

/* LEDs spacing */
.level::after {
  width: 100%;
  height: 100%;
  content: " ";
  position: absolute;
  top: 0px;
  left: 0px;
  z-index: 2;
  margin-top: 3px;
  background-image: linear-gradient(
    to bottom,
    #333 1px /* space between leds*/,
    transparent 1px
  );
  background-repeat: repeat-y;
  background-size: 100% 4px; /* 4px LED height */
  background-color: transparent;
}

/* LOADER */

.loader-wrap {
  display: flex;
  flex-direction: column;
  width: 100%;
  justify-content: flex-end;
}

.loader-wrap span {
  font-size: 2.5rem;
  text-align: center;
}

.loader-container {
  display: grid;
  place-items: center;
  min-height: 50vh;
}
.lds-roller {
  display: inline-block;
  position: relative;
  width: 80px;
  height: 80px;
  transform: scale(250%);
}
.lds-roller div {
  animation: lds-roller 1.2s cubic-bezier(0.5, 0, 0.5, 1) infinite;
  transform-origin: 40px 40px;
}
.lds-roller div:after {
  content: " ";
  display: block;
  position: absolute;
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background: #fff;
  margin: -4px 0 0 -4px;
}
.lds-roller div:nth-child(1) {
  animation-delay: -0.036s;
}
.lds-roller div:nth-child(1):after {
  top: 63px;
  left: 63px;
}
.lds-roller div:nth-child(2) {
  animation-delay: -0.072s;
}
.lds-roller div:nth-child(2):after {
  top: 68px;
  left: 56px;
}
.lds-roller div:nth-child(3) {
  animation-delay: -0.108s;
}
.lds-roller div:nth-child(3):after {
  top: 71px;
  left: 48px;
}
.lds-roller div:nth-child(4) {
  animation-delay: -0.144s;
}
.lds-roller div:nth-child(4):after {
  top: 72px;
  left: 40px;
}
.lds-roller div:nth-child(5) {
  animation-delay: -0.18s;
}
.lds-roller div:nth-child(5):after {
  top: 71px;
  left: 32px;
}
.lds-roller div:nth-child(6) {
  animation-delay: -0.216s;
}
.lds-roller div:nth-child(6):after {
  top: 68px;
  left: 24px;
}
.lds-roller div:nth-child(7) {
  animation-delay: -0.252s;
}
.lds-roller div:nth-child(7):after {
  top: 63px;
  left: 17px;
}
.lds-roller div:nth-child(8) {
  animation-delay: -0.288s;
}
.lds-roller div:nth-child(8):after {
  top: 56px;
  left: 12px;
}
@keyframes lds-roller {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

/* FX PANEL */

.fx-panels-wrap {
  position: absolute;
  display: flex;
  width: 0;
  gap: 9px;
  height: 50%;
  justify-content: center;
  align-items: center;
  flex-wrap: nowrap;
}

.panel {
  position: relative;
  cursor: move;
  color: black;
  border-radius: 5px;
  padding: 12px;
  user-select: none;
  background: var(--yellowLinearBtmTransparent);
  z-index: 999;
  box-shadow: var(--shadow-elevation-high);
}

.panel:nth-of-type(5n + 1) {
  background: var(--pink);
}

.panel:nth-of-type(5n + 2) {
  background: var(--blue);
}

.panel:nth-of-type(5n + 3) {
  background: var(--yellow);
}

.panel:nth-of-type(5n + 4) {
  background: var(--red);
}

.panel:nth-of-type(5n + 5) {
  background: var(--green);
}

.button {
  width: fit-content;
  margin: 0;
  padding: 4px;
  border-radius: 4px;
  border: none;
  background: none;
  color: inherit;
  white-space: nowrap;
  display: flex;
  justify-content: center;
  align-items: center;
}

.link-btn {
  font-family: "Tekton";
  font-weight: 700;
  font-size: 1.3rem;
  gap: 12px;
}

nav a:hover,
.link-btn:hover,
.link-btn svg:hover {
  color: var(--pink);
}

nav a:active,
.link-btn:active {
  color: var(--blue);
}

button:disabled,
.opacity-05 {
  opacity: 0.25;
}

.open {
  display: flex;
}

.closed {
  display: none;
}
.effect-select {
  font-weight: bold;
  font-size: 1.2rem;
  letter-spacing: 0.05rem;
  background: var(--blue);
  border: none;
  color: #fff;
  width: 104px;
  padding: 4px;
  border-radius: 4px;
  outline: none;
  text-align: center;
  font-family: "Tekton";
  position: relative;
  left: 2px;
}

.fx-toggle {
  display: flex;
  flex-direction: column;
  gap: 4px;
}

.fx-toggle-btn {
  width: 100%;
  font-family: "Tekton";
  background: var(--blue);
  padding: 4px;
}

.fx-panel-btn {
  position: absolute;
  align-self: flex-start;
  right: 8px;
  top: 8px;
  background: #121212;
  color: white;
  padding: 4px;
  scale: 0.75;
  border-radius: 4px;
}

.badge {
  font-size: 0.5rem;
  position: absolute;
  top: -2.5em;
  left: 0;
  width: fit-content;
  color: var(--yellow);
}

.mix-credits {
  display: flex;
  flex-direction: column;
  gap: 8px;
}

.likes {
  position: relative;
  display: flex;
}

.likes > span {
  font-family: "Tekton";
  font-weight: 700;
  font-size: 0.75rem;
  position: absolute;
  top: 18px;
  color: var(--transparent-white);
}
.square {
  width: 60px;
  height: 60px;
  display: grid;
  place-items: center;
}

.red {
  background: var(--redLinearBtm);
}
.red:active {
  box-shadow: inset 0px 3px 6px hsla(0, 0%, 0%, 0.25);
  background: var(--redLinearTop);
}
.red svg {
  fill: #dedede;
  stroke: #dedede;
}
.buttons-wrap {
  display: flex;
  gap: 4px;
}

.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  border: 0;
}

.hidden {
  display: none;
}

.colored {
  padding: 0;
  border-radius: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.colored div {
  display: flex;
  align-items: center;
  justify-content: center;
  aspect-ratio: 1/1;
  background: var(--transparent-white);
  padding: 12px;
  height: 100%;
}

.colored svg {
  scale: 1.25;
}
.colored > span {
  color: var(--transparent-white);
  position: relative;
  top: 3px;
  font-weight: 700;
  font-size: 1.5rem;
  font-family: "Tekton";
  letter-spacing: 0.1rem;
  padding: 9px 12px;
}

.colored:nth-of-type(1) {
  background: var(--red);
}

.colored:nth-of-type(1) svg {
  stroke: var(--red);
}

.colored:nth-of-type(2) svg {
  stroke: var(--blue);
}

.colored:nth-of-type(2) {
  background: var(--blue);
}

.hover-img {
  background-color: #000;
  color: #fff;
  display: inline-block;
  margin: 8px;
  max-width: 320px;
  min-width: 240px;
  overflow: hidden;
  position: relative;
  width: 100%;
}

.hover-img * {
  transition: all 0.45s ease;
}

.hover-img::before,
.hover-img::after {
  background-color: rgba(0, 0, 0, 0.5);
  border-top: 32px solid rgba(0, 0, 0, 0.5);
  border-bottom: 32px solid rgba(0, 0, 0, 0.5);
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  content: "";
  transition: all 0.3s ease;
  z-index: 1;
  opacity: 0;
  transform: scaleY(2);
}

.hover-img img {
  vertical-align: top;
  max-width: 100%;
  backface-visibility: hidden;
}

.hover-img figcaption {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 38px;
  right: 0;
  opacity: 0;
  z-index: 2;
  display: grid;
  grid-template-rows: 10% 80%;
  place-items: center;
  transition-delay: 0.1s;
  max-width: 16ch;
  width: fit-content;
  font-weight: 700;
  color: #dedede;
  & a {
    font-weight: 700;
    color: #fff;
    font-size: 1.1rem;
  }
}

.hover-img header {
  position: relative;
  top: 10px;
}

.hover-img:hover::before,
.hover-img:hover::after {
  transform: scale(1);
  opacity: 1;
}

.hover-img:hover > img {
  opacity: 0.7;
}

.hover-img:hover figcaption {
  opacity: 1;
}

/* HOVER ANIMATION */
.card {
  width: 300px;
  height: 280px;
  padding: 0;
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.1);
  border-radius: 2px;
  box-sizing: border-box;
  overflow: hidden;
  background: #020202;
}

.card * {
  transition: 0.3s ease all;
}

.card img {
  margin: 0;
  width: 300px;
  height: 224px;
  object-fit: cover;
  display: block;
}

.card h3 {
  margin: 0;
  padding: 12px 12px 48px;
  line-height: 32px;
  font-weight: 500;
  font-size: 20px;
}

.card .focus-content {
  display: block;
  padding: 8px 12px;
}

.card p {
  margin: 0;
  line-height: 1.5;
}

.card:active img,
.card:hover img,
.card:focus-within img {
  margin-top: -80px;
}

.card:hover h3,
.card:focus-within h3 {
  padding: 8px 12px 0;
}
